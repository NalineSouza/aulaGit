programa {
  funcao inicio() {
    real diagonalmenor, diagonalmaior, area
    escreva("DIGITE O VALOR DA DIAGONALMENOR: ")
    leia(diagonalmenor)    
    escreva("DIGITE O VALOR DA DIAGONALMAIOR: ")
    leia(diagonalmaior)
    area = (diagonalmenor * diagonalmaior) / 2
    escreva("O RESULTADO É: ", area)
  }
}
